<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.project.demos.web.dao.GuaFinancialDebtDetailDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.example.project.demos.web.entity.GuaFinancialDebtDetailEntity" id="guaFinancialDebtDetailMap">
        <result property="id" column="id"/>
        <result property="debtId" column="debt_id"/>
        <result property="projectName" column="project_name"/>
        <result property="projectCode" column="project_code"/>
        <result property="countryAmount" column="country_amount"/>
        <result property="otherAmount" column="other_amount"/>
        <result property="tollAmount" column="toll_amount"/>
    </resultMap>

    <select id="queryList" resultMap="guaFinancialDebtDetailMap">
        select project_name,project_code,country_amount,other_amount,toll_amount
        from gua_financial_debt_detail where debt_id = #{debtId}
    </select>
</mapper>